<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:camel="http://camel.apache.org/schema/spring"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
           http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

<import resource="classpath:route/camel-context.xml"/>

    <bean id="fixedLengthDecoder" class="org.example.FixedLengthFrameDecoder">
        <constructor-arg value="256"/>
    </bean>

    <bean id="fixedLengthEncoder" class="org.example.FixedLengthFrameEncoder"/>

    <bean id="fixedLengthProcessor" class="org.example.FixedLengthProcessor"/>

    <bean id="timeoutProcessor" class="org.example.TimeoutProcessor"/>

</beans>
